{
  "name": "arrow",
  "version-string": "7.0.0-SNAPSHOT",
  "description": "Cross-language development platform for in-memory analytics",
  "homepage": "https://arrow.apache.org",
  "supports": "x64 | (arm64 & !windows)",
  "dependencies": [
    "boost-multiprecision",
    "boost-system",
    "boost-filesystem",
    "boost-process",
    "brotli",
    "bzip2",
    "curl",
    "flatbuffers",
    "gflags",
    "glog",
    "lz4",
    "openssl",
    "re2",
    "snappy",
    "utf8proc",
    "zlib",
    "zstd",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "dev",
    "flight",
    "gcs",
    "json",
    "orc",
    "parquet",
    "s3"
  ],
  "features": {
    "dev": {
      "description": "Development dependencies",
      "dependencies": [
        "benchmark",
        "gtest"
      ]
    },
    "flight": {
      "description": "Flight RPC support",
      "dependencies": [
        "abseil",
        "c-ares",
        "grpc",
        "protobuf",
        "sqlite3"
      ]
    },
    "json": {
      "description": "JSON support",
      "dependencies": [
        "rapidjson"
      ]
    },
    "gandiva": {
      "description": "Gandiva support",
      "dependencies": [
        "boost-crc",
        "llvm"
      ]
    },
    "gcs": {
      "description": "GCS support",
      "dependencies": [
        {
          "name": "google-cloud-cpp",
          "default-features": false,
          "features": [
            "storage"
          ]
        }
      ]
    },
    "orc": {
      "description": "ORC support",
      "dependencies": [
        "orc"
      ]
    },
    "parquet": {
      "description": "Parquet support",
      "dependencies": [
        "thrift"
      ]
    },
    "s3": {
      "description": "S3 support",
      "dependencies": [
        {
          "name": "aws-sdk-cpp",
          "default-features": false,
          "version>=": "1.8.126",
          "features": [
            "cognito-identity",
            "config",
            "identity-management",
            "s3",
            "sts",
            "transfer"
          ]
        }
      ]
    }
  },
  "overrides": [
    { "name": "gtest", "version": "1.10.0", "port-version": 4 },
    { "name": "aws-sdk-cpp", "version": "1.8.126", "port-version": 10 }
  ],
  "builtin-baseline": "b5865bfeba430cd44063fc54a45a8861195a715f"
}
