[package]
name = "arrow-dependencies"
version = "0.0.0"

[dependencies]
brotli = "2.5"
byteorder = "1"
chrono = "0.4.7"
clap = "2.33.0"
crossbeam = "0.7.1"
csv = "1.0"
indexmap = "1.0"
flatbuffers = "0.5.0"
flate2 = "1.0.2"
fnv = "1.0.3"
lazy_static = "1.2"
libc = "0.2"
lz4 = "1.23"
num = "0.2"
num-bigint = "0.2"
packed_simd = { version = "0.3.1", optional = true }
parquet-format = "~2.5"
prettytable-rs = "0.8.0"
quick-error = "1.2.2"
rand = "0.6"
regex = "1.1"
rustyline = {version = "4.1.0", optional = true}
serde = { version = "1.0.80", features = ["rc"] }
serde_derive = "1.0.80"
serde_json = { version = "1.0.13", features = ["preserve_order"] }
snap = "0.2"
sqlparser = "0.2.0"
thrift = "0.0.4"
zstd = "0.4"